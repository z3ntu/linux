&i2c1 {
	status = "okay";

	#include "pm8008.dtsi"

	typec-mux@42 {
		compatible = "ocs,ocp96011", "fcs,fsa4480";
		reg = <0x42>;

		interrupts-extended = <&tlmm 7 IRQ_TYPE_LEVEL_LOW>;

		vcc-supply = <&vreg_bob>;

		mode-switch;
		orientation-switch;

		port {
			ocp96011_sbu_mux: endpoint {
				remote-endpoint = <&pmic_glink_sbu>;
				data-lanes = <1 0>;
